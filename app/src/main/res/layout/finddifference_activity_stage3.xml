<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/spot_the_difference_game_background1"
    >

    <Button
        android:id="@+id/restar"
        android:layout_width="96dp"
        android:layout_height="65dp"
        android:layout_weight="1"
        android:text="@string/next3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="226dp"
        android:layout_height="60dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/content3"
        android:textColor="#348feb"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/score"
        android:layout_width="193dp"
        android:layout_height="59dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/score3"
        android:textColor="#348feb"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/stage1"
        android:layout_width="300dp"
        android:layout_height="260dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.406"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.983"
        app:srcCompat="@drawable/stage6"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/stage2"
        android:layout_width="293dp"
        android:layout_height="257dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.206"
        app:srcCompat="@drawable/stage5"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ans21"
        android:layout_width="40dp"
        android:layout_height="49dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintVertical_bias="0.867"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans22"
        android:layout_width="32dp"
        android:layout_height="48dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage2"
        app:layout_constraintEnd_toEndOf="@+id/stage2"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="@+id/stage2"
        app:layout_constraintTop_toTopOf="@+id/stage2"
        app:layout_constraintVertical_bias="0.583"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans31"
        android:layout_width="64dp"
        android:layout_height="57dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage1"
        app:layout_constraintEnd_toEndOf="@+id/stage1"
        app:layout_constraintHorizontal_bias="0.991"
        app:layout_constraintStart_toStartOf="@+id/stage1"
        app:layout_constraintTop_toTopOf="@+id/stage1"
        app:layout_constraintVertical_bias="0.221"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans32"
        android:layout_width="66dp"
        android:layout_height="67dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage2"
        app:layout_constraintEnd_toEndOf="@+id/stage2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/stage2"
        app:layout_constraintTop_toTopOf="@+id/stage2"
        app:layout_constraintVertical_bias="0.226"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans11"
        android:layout_width="25dp"
        android:layout_height="62dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage1"
        app:layout_constraintEnd_toEndOf="@+id/stage1"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="@+id/stage1"
        app:layout_constraintTop_toTopOf="@+id/stage1"
        app:layout_constraintVertical_bias="0.227"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans12"
        android:layout_width="25dp"
        android:layout_height="48dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage2"
        app:layout_constraintEnd_toEndOf="@+id/stage2"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="@+id/stage2"
        app:layout_constraintTop_toTopOf="@+id/stage2"
        app:layout_constraintVertical_bias="0.205"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans41"
        android:layout_width="19dp"
        android:layout_height="20dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage1"
        app:layout_constraintEnd_toEndOf="@+id/stage1"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/stage1"
        app:layout_constraintVertical_bias="0.4"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans42"
        android:layout_width="17dp"
        android:layout_height="15dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage2"
        app:layout_constraintEnd_toEndOf="@+id/stage2"
        app:layout_constraintHorizontal_bias="0.612"
        app:layout_constraintStart_toStartOf="@+id/stage2"
        app:layout_constraintTop_toTopOf="@+id/stage2"
        app:layout_constraintVertical_bias="0.376"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans51"
        android:layout_width="41dp"
        android:layout_height="67dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage2"
        app:layout_constraintEnd_toEndOf="@+id/stage2"
        app:layout_constraintHorizontal_bias="0.912"
        app:layout_constraintStart_toStartOf="@+id/stage2"
        app:layout_constraintTop_toTopOf="@+id/stage2"
        app:layout_constraintVertical_bias="0.757"
        app:srcCompat="@drawable/invisible" />

    <ImageView
        android:id="@+id/ans52"
        android:layout_width="45dp"
        android:layout_height="64dp"

        app:layout_constraintBottom_toBottomOf="@+id/stage1"
        app:layout_constraintEnd_toEndOf="@+id/stage1"
        app:layout_constraintHorizontal_bias="0.925"
        app:layout_constraintStart_toStartOf="@+id/stage1"
        app:layout_constraintTop_toTopOf="@+id/stage1"
        app:layout_constraintVertical_bias="0.765"
        app:srcCompat="@drawable/invisible" />

</androidx.constraintlayout.widget.ConstraintLayout>
